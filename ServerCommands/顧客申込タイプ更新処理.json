{
  "Enabled": true,
  "Triggers": [
    {
      "$type": "Forguncy.ServerCommands.Models.PostRequestTrigger, ServerDesignerCommon",
      "Permission": {
        "AllowRoles": [
          "FGC_LoginUser"
        ]
      },
      "Parameters": [
        {
          "Name": "P_顧客ID"
        },
        {
          "Type": 1,
          "ArrayParamItems": [
            {
              "Name": "P_グループID"
            },
            {
              "Name": "P_申込タイプID"
            },
            {
              "Name": "P_初期セットフラグ"
            },
            {
              "Name": "P_非公開フラグ"
            },
            {
              "Name": "P_有効フラグ"
            }
          ],
          "PrimaryKeys": [
            "ID"
          ],
          "Name": "P_顧客申込タイプビュー"
        }
      ]
    }
  ],
  "Commands": [
    {
      "$type": "Forguncy.Model.ServerTransactionCommand, ServerDesignerCommon",
      "CommandList": [
        {
          "$type": "Forguncy.Model.LoopCommand, ServerDesignerCommon",
          "LoopInfo": {
            "$type": "Forguncy.Model.CountLoopInfo, ServerDesignerCommon",
            "LoopCount": {
              "$type": "Forguncy.Model.FormulaReferObject, ServerDesignerCommon",
              "SerializeProperty": "=P_顧客申込タイプビュー"
            },
            "LoopItemParamName": "Item"
          },
          "CommandList": [
            {
              "$type": "Forguncy.Model.UpdateDataTableCommand, ServerDesignerCommon",
              "TableName": "m_customer_order_type",
              "ShowConfirm": false,
              "ConfirmText": "本当に更新してもよろしいですか？",
              "RowsToUpdate": 1,
              "RowsToUpdateCondition": {
                "$type": "ForguncyDataAccess.RelationSqlCondition, ForguncyDataAccess",
                "Condition1": {
                  "$type": "ForguncyDataAccess.RelationSqlCondition, ForguncyDataAccess",
                  "Condition1": {
                    "$type": "ForguncyDataAccess.GeneralCESqlCondition, ForguncyDataAccess",
                    "ColumnBindingInfo": {
                      "TableName": "m_customer_order_type",
                      "ColumnName": "customer_id"
                    },
                    "Value": {
                      "$type": "Forguncy.Model.FormulaReferObject, ServerDesignerCommon",
                      "SerializeProperty": "=P_顧客ID"
                    }
                  },
                  "Condition2": {
                    "$type": "ForguncyDataAccess.GeneralCESqlCondition, ForguncyDataAccess",
                    "ColumnBindingInfo": {
                      "TableName": "m_customer_order_type",
                      "ColumnName": "group_id"
                    },
                    "Value": {
                      "$type": "Forguncy.Model.FormulaReferObject, ServerDesignerCommon",
                      "SerializeProperty": "=Item.P_グループID"
                    }
                  }
                },
                "Condition2": {
                  "$type": "ForguncyDataAccess.GeneralCESqlCondition, ForguncyDataAccess",
                  "ColumnBindingInfo": {
                    "TableName": "m_customer_order_type",
                    "ColumnName": "order_type_id"
                  },
                  "Value": {
                    "$type": "Forguncy.Model.FormulaReferObject, ServerDesignerCommon",
                    "SerializeProperty": "=Item.P_申込タイプID"
                  }
                }
              },
              "UpdateValues": [
                {
                  "ColumnName": "default_set_flg",
                  "Value": {
                    "$type": "Forguncy.Model.FormulaReferObject, ServerDesignerCommon",
                    "SerializeProperty": "=Item.P_初期セットフラグ"
                  }
                },
                {
                  "ColumnName": "private_flg",
                  "Value": {
                    "$type": "Forguncy.Model.FormulaReferObject, ServerDesignerCommon",
                    "SerializeProperty": "=Item.P_非公開フラグ"
                  }
                },
                {
                  "ColumnName": "active_flg",
                  "Value": {
                    "$type": "Forguncy.Model.FormulaReferObject, ServerDesignerCommon",
                    "SerializeProperty": "=Item.P_有効フラグ"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "Name": "顧客申込タイプ更新処理"
}